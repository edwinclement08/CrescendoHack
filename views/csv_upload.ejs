<%- include partials/header.ejs %>
<header id="fh5co-header" class="fh5co-cover fh5co-cover-sm" role="banner"
	style="background-image:url(images/img_bg_1.jpg);">
	<div class="overlay"></div>
	<div class="fh5co-container">
		<div class="row">
			<div class="col-md-8 col-md-offset-2 text-center">
				<div class="display-t">
					<div class="display-tc animate-box" data-animate-effect="fadeIn">
						<h1>Upload CSV</h1>

						<h2>Welcome!!
						</h2>
					</div>
				</div>
			</div>
		</div>
	</div>
</header>
<style>
	td,
	th {

		padding: 0;
		background: white;
		color: blueviolet;

	}
</style>
<div class="fh5co-container animate-box col-md-offset-4" style="margin-top:5em;">
	<form class="form-inline" method="POST" action="/upload_bill_csv"  enctype="multipart/form-data">
		<div class="row" style="margin:5px;align-content: center" >
			<input type="file" name="filetoupload" id="bill"  placeholder="Add Image of Bill"
				style="align-content: center;">
		</div>
		<div class="row"style="margin:5px;align-content: center">
			<button type="submit" class="btn btn-default btn-block"
				style="background-color: blueviolet;margin:5px;width:20vw">Confirm</button>
		</div>
	</form>
	<!-- <progress></progress> -->
</div>
<script src="http://ajax.googleapis.com/ajax/libs/jquery/1.7/jquery.js"></script>
<script src="http://malsup.github.com/jquery.form.js"></script>
<%- include partials/footer.ejs %>