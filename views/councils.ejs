<%- include partials/header.ejs %>

<header id="fh5co-header" class="fh5co-cover fh5co-cover-sm" role="banner"
	style="background-image:url(images/img_bg_1.jpg);">
	<div class="overlay"></div>
	<div class="fh5co-container">
		<div class="row">
			<div class="col-md-8 col-md-offset-2 text-center">
				<div class="display-t">
					<div class="display-tc animate-box" data-animate-effect="fadeIn">
						<h1>Council Page</h1>
						<h2>Welcome!!
						</h2>
					</div>
				</div>
			</div>
		</div>
	</div>
</header>

<div id="fh5co-couple-story">
	<div class="container">
		<div class="row">
			<div class="col-md-8 col-md-offset-2 text-center fh5co-heading animate-box">
				<span></span>
				<h2 style="color:blueviolet">Upload your Event Idea</h2>
				<p>Far far away, behind the word mountains, far from the countries Vokalia and Consonantia,
					there live the blind texts.</p>
			</div>
		</div>
		<div class="row animate-box">
			<div class="col-md-12 col-md-offset-0">
				<div class="col-md-10 col-md-offset-1">
					<form class="form-inline">
						<div class="col-md-8 col-sm-8 col-md-offset-2" style="margin:auto">
							<div class="form-group">
								<!-- <label for="name" class="sr-only">Name</label> -->
								<input type="name" class="form-control" id="name" placeholder="Name">
							</div>
						</div>
						<div class="col-md-10 col-sm-10">
							<div class="form-group" style="margin:auto">
								<textarea name="description" class="form-control" cols="47" rows="10"
									placeholder="Description"></textarea>
							</div>
						</div>
						<div class="col-md-10 col-sm-10">
							<div class="btn-group btn-group-toggle" style="margin-left:17rem" data-toggle="buttons">
								<label class="btn btn-secondary active">
									<input type="radio" name="options" id="option1" width="25rem" autocomplete="off" checked> 601
								</label>
								<label class="btn btn-secondary">
									<input type="radio" name="options" id="option2" width="25rem" autocomplete="off"> 602
								</label>
								<label class="btn btn-secondary">
									<input type="radio" name="options" id="option3" width="25rem" autocomplete="off"> 603
								</label>
								<label class="btn btn-secondary">
									<input type="radio" name="options" id="option2" width="25rem" autocomplete="off"> 604
								</label>
							</div>
						</div>
						
						<div class="col-md-4 col-sm-4" style="margin:10px">
							<button type="submit" class="btn btn-default btn-block">Submit</button>
						</div>
					</form>
				</div>
			</div>
		</div>
	</div>
</div>


<%- include partials/footer.ejs %>